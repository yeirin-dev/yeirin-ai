# ============================================
# Yeirin-AI Backend - Development Server Configuration
# Values loaded from AWS Parameter Store: /yeirin/dev/*
# ============================================

# Database Configuration (Read-Only from yeirin_dev)
DATABASE_URL=postgresql://${DB_USERNAME}:${DB_PASSWORD}@${DB_HOST}:5432/yeirin_dev

# OpenAI Configuration (from Parameter Store: /yeirin/dev/common/openai-api-key)
OPENAI_API_KEY=${OPENAI_API_KEY}
OPENAI_MODEL=gpt-4o-mini

# Application Configuration
APP_NAME=yeirin-ai
APP_VERSION=0.1.0
DEBUG=true
LOG_LEVEL=INFO
PORT=8001

# API Configuration
API_V1_PREFIX=/api/v1
CORS_ORIGINS=["http://localhost:3000","http://yeirin:3000"]

# MSA Integration
YEIRIN_BACKEND_URL=http://yeirin:3000
SOUL_E_WEBHOOK_URL=http://soul-e:8000
INTERNAL_API_SECRET=${INTERNAL_API_SECRET}
